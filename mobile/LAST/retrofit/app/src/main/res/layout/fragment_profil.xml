<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfilFragment"
    android:background="@drawable/profil"
    >

    <!-- Rectangle -->
    <View
        android:id="@+id/rectangle_6"
        android:layout_width="match_parent"
        android:layout_height="690dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="167dp"
        android:background="@drawable/rectangle_6" />

    <View
        android:id="@+id/group_14"
        android:layout_width="157dp"
        android:layout_height="148dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="69dp"
        android:background="@drawable/group_14" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/etProfilName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/group_14"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/lato"
        android:text="Name"
        android:textColor="@color/black"
        android:textSize="25sp"
        />
<!--    <TextView-->
<!--        android:id="@+id/etName"-->
<!--        android:layout_width="wrap_content"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:layout_below="@+id/group_14"-->
<!--        android:layout_centerHorizontal="true"-->
<!--        android:layout_marginTop="10dp"-->
<!--        android:fontFamily="@font/lato"-->
<!--        android:text="Name"-->
<!--        android:textColor="@color/black"-->
<!--        android:textSize="25sp" />-->

    <TextView
        android:id="@+id/etProfilKelas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/etProfilName"
        android:text="Kelas"
        android:fontFamily="@font/lato"
        android:textSize="18sp"
        android:textColor="#BF8C00"/>

    <!-- Keluar -->
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_keluar"
        android:layout_width="386dp"
        android:layout_height="49dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="600dp"
        android:background="@drawable/bg_buttonkeluar"
        android:onClick="logout"
        android:fontFamily="@font/lato"
        android:text="@string/keluar"
        android:textColor="#FF0000" />

    <!-- Rectangle 90 -->
    <View
        android:id="@+id/rectangle_9"
        android:layout_width="434dp"
        android:layout_height="118dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="-3.5dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="294dp"
        android:background="@drawable/rectangle_9"
        />

    <!-- Vector -->
    <View
        android:id="@+id/vector"
        android:layout_width="100dp"
        android:layout_height="73dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="317dp"
        android:layout_marginLeft="40dp"
        android:background="@drawable/vector"
        />

    <!-- Jangan Lupa bayar kas dan mengerjakan tugas !!!! -->
    <TextView
        android:id="@+id/jangan_lupa"
        android:layout_width="200dp"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="165dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="333dp"
        android:text="@string/jangan_lupa"
        android:textAppearance="@style/jangan_lupa"
        android:gravity="top"
        android:fontFamily="@font/poppins"
        />

</RelativeLayout>