<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfilFragment"
    android:background="@drawable/profil"
    android:id="@+id/fragmentProfil">

    <!-- Rectangle -->
    <View
        android:id="@+id/rectangle_6"
        android:layout_width="match_parent"
        android:layout_height="690dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="167dp"
        android:background="@drawable/rectangle_6" />

    <View
        android:id="@+id/group_14"
        android:layout_width="157dp"
        android:layout_height="148dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="69dp"
        android:background="@drawable/group_14" />

    <TextView
        android:id="@+id/etProfilName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/group_14"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="Name"
        android:fontFamily="@font/lato"
        android:textSize="25sp"
        android:textColor="@color/black"/>

    <TextView
        android:id="@+id/etProfilKelas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/etProfilName"
        android:text="Kelas"
        android:fontFamily="@font/lato"
        android:textSize="18sp"
        android:textColor="#BF8C00"/>

    <!-- Keluar -->
    <TextView
        android:id="@+id/keluar"
        android:layout_width="52dp"
        android:layout_height="22dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="600dp"
        android:text="@string/keluar"
        android:textAppearance="@style/keluar" />


</RelativeLayout>